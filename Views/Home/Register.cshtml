@{
    ViewData["Title"] = "Registrarse";
}

<form method="post" action="RegisterAction" enctype="multipart/form-data">
    <input class="form-control" type="text" name="Nombre" id="Nombre" placeholder="Nombre de usuario..." required>
    <input class="form-control" type="password" name="Contrasena" id="Contrasena" placeholder="ContraseÃ±a..." required>
    <input class="form-control" type="email" name="Mail" id="Mail" placeholder="Correo electronico..." required>
    <input class="form-control" type="date" name="FechaNacimiento" id="FechaNacimiento" placeholder="Fecha de nacimiento..." required>
    <input class="form-control" type="file" name="MyFile" id="MyFile" placeholder="Foto de perfil...">
    <button class="btn btn-primary" type="submit">Registrarse</button>
</form>

<script>
